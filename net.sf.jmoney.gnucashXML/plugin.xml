<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="net.sf.jmoney.gnucashXML"
            visible="true"
            id="net.sf.jmoney.gnucashXML">
         <menu
               label="%MainFrame.import"
               id="importMenu">
            <separator name="importExportGroup"/>
         </menu>
         <action
               label="%MainFrame.import"
               class="net.sf.jmoney.gnucashXML.actions.GnucashXMLImportAction"
               tooltip="Import Data from a GnuCash XML file"
               icon="icons/Import16.gif"
               menubarPath="file/importExportGroup"
               id="net.sf.jmoney.gnucashXML.actions.gnucashXMLImport"/>
         <action
               label="%MainFrame.export"
               class="net.sf.jmoney.gnucashXML.actions.GnucashXMLExportAction"
               tooltip="Export Data into a GnuCash XML file"
               icon="icons/Export16.gif"
               menubarPath="file/importExportGroup"
               id="net.sf.jmoney.gnucashXML.actions.gnucashXMLExport"/>
      </actionSet>
   </extension>

</plugin>
