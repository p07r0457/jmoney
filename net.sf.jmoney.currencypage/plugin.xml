<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="net.sf.jmoney.currencypage"
   name="Currency Page Plug-in"
   version="1.0.0"
   provider-name="JMoney"
   class="net.sf.jmoney.currencypage.CurrencyPagePlugin">

   <runtime>
      <library name="currencypage.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="net.sf.jmoney"/>
      <import plugin="org.eclipse.ui.forms"/>
   </requires>

   <extension
         point="net.sf.jmoney.pages">
      <node
          id="currencyNode"
	      label="%NavigationTreeModel.currencies"
    	  icon="icons/currencies.gif"
    	  position="210">
      </node>
      <pages
            node="net.sf.jmoney.currencypage.currencyNode"
            class="net.sf.jmoney.currencypage.CurrencyPage"
            id="currencies">
      </pages>
   </extension>

</plugin>
