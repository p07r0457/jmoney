<schema targetNamespace="net.sf.jmoney">
<annotation>
      <appInfo>
         <meta.schema plugin="net.sf.jmoney" id="pages" name="Bookkeeping Pages"/>
      </appInfo>
      <documentation>
         Add tabbed pages to the &apos;Folder&apos; view.  
         A page may be specific to any object type that is listed
         in the navigation view.  When an object is selected in the
         navigation view, all appropriate tabbed pages that have 
         been added through this extension point will be shown in 
         the tabbed view.
         Filtering may be done not only on the object type, 
         but also on the object id and also may be filtered 
         based on the value of properties in the selected object.
         If a node element is specified then a node will be added
         to the navigation view if one does not already exist.
         A node may only be added if the page is filtered on node
         id (the nodeId attribute is specified on the page element).
      </documentation>
   </annotation>

   <element name="extension">
      <complexType>
         <sequence>
         </sequence>
         <attribute name="point" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="pages">
      <complexType>
         <attribute name="class" type="string">
            <annotation>
               <documentation>
                  Class must implement IBookkeepingPages.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="node" type="string">
            <annotation>
               <documentation>
                  The fully qualified id of a node.  This page
                  will be shown whenever the node of the given
                  id is selected.
                  This attribute is optional.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="extendable-property-set" type="string">
            <annotation>
               <documentation>
                  The fully qualified id of an extendable property set.
                  Extension property sets may not be specified.
                  The page will be shown whenever a node is selected
                  that represents an object that contains the given
                  property set.
                  This attribute is optional.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="node">
      <complexType>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  The id of the node.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="label" type="string">
            <annotation>
               <documentation>
                  The label to be displayed in the navigation tree.
     If a node already exists with the same id but the
     label is different then a separate node will be shown.
     Selecting either node will cause the same pages to
     be shown.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="icon" type="string">
            <annotation>
               <documentation>
                  The icon to be displayed in the navigation
                  view.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="parent" type="string">
            <annotation>
               <documentation>
                  The id of the parent node.
     If this attribute is empty or is not specified
     then the node added to the root node.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="position" type="integer">
            <annotation>
               <documentation>
                  A number in the range 0 to 1000.
                  Nodes with the same parent are sorted according
                  to this number.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

