      <complexType>
         <sequence>
            <element ref="file-format"/>
         </sequence>
         <attribute name="point" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="file-format">
      <complexType>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  The id of this file format.
                  The globally unique id of the plug-in defining the
                  file format is prepended, so this id need be unique
                  only within the plugin.xml file.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="file-extension" type="string">
            <annotation>
               <documentation>
                  The file extension used for this file format.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="format-description" type="string">
            <annotation>
               <documentation>
                  The localized description.  This is displayed by the
open dialog and save dialog in the drop-down list
of file types.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="class" type="string">
            <annotation>
               <documentation>
                  The name of a class that implements the IFileDatastore
interface.  This class provides implementations of the
methods to open and to save the file.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

